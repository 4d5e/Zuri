{
   "name": "reinfyteam/zuri-rewrite",
   "description": "Zuri is an anticheat made to protect the server from any may unfair advantages from the players.",
   "authors": [
      {
         "name": "ReinfyTeam",
         "email": "support@reinfyteam.tk"
      }
   ],
   "require": {
      "pocketmine/pocketmine-mp": "^5.4.4",
      "phpstan/phpstan": "^1.10.0",
      "phpstan/phpstan-strict-rules": "^1.0",
      "phpstan/extension-installer": "^1.0"
   },
   "require-dev": {
      "pocketmine/pocketmine-mp": "^5.4.4"
   },
   "autoload": {
      "psr-4": {
         "Zuri\\": "../src"
      }
   },
   "config": {
      "allow-plugins": {
         "phpstan/extension-installer": true
      }
   },
   "repositories": [
      {
         "type": "vcs",
         "url": "https://github.com/pmmp/PocketMine-MP.git"
      }
   ]
}
